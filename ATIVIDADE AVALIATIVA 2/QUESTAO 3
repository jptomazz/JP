#include <stdio.h>
#include <string.h>

int main() {
    int matrizA[9][9], matrizB[9][9], valorFinal[9][9];
    char operacao[5];

    //primeira matriz
    for (int linha = 0; linha < 9; linha++) {
        for (int coluna = 0; coluna < 9; coluna++) {
            scanf("%d", &matrizA[linha][coluna]);
        }
    }

    //segunda matriz
    for (int linha = 0; linha < 9; linha++) {
        for (int coluna = 0; coluna < 9; coluna++) {
            scanf("%d", &matrizB[linha][coluna]);
        }
    }

    //operação
    scanf("%s", operacao);

    //operação e calcular o valorFinal
    for (int linha = 0; linha < 9; linha++) {
        for (int coluna = 0; coluna < 9; coluna++) {
            if (strcmp(operacao, "soma") == 0) {
                valorFinal[linha][coluna] = matrizA[linha][coluna] + matrizB[linha][coluna];
            } else if (strcmp(operacao, "sub") == 0) {
                valorFinal[linha][coluna] = matrizA[linha][coluna] - matrizB[linha][coluna];
            } else if (strcmp(operacao, "mult") == 0) {
                valorFinal[linha][coluna] = 0;
                for (int operMult = 0; operMult < 9; operMult++) {
                    valorFinal[linha][coluna] += matrizA[linha][operMult] * matrizB[operMult][coluna];
                }
            }
        }
    }

    // valorFinal formatado
    for (int linha = 0; linha < 9; linha++) {
        for (int coluna = 0; coluna < 9; coluna++) {
            printf("%9d", valorFinal[linha][coluna]);
        }
        printf("\n");
    }

    return 0;
}